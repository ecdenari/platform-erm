# Production Environment Configuration
# Environment variables for production deployment

# API Configuration
VITE_API_BASE_URL=https://api.fieldpoint.com
VITE_API_TIMEOUT=8000

# Environment
VITE_ENVIRONMENT=production

# Authentication
VITE_JWT_TOKEN_KEY=fieldpoint_auth_token

# Application Settings
VITE_APP_NAME=Fieldpoint Platform
VITE_APP_VERSION=2.0.0

# Debug Settings (disabled in production)
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# Feature Flags
VITE_ENABLE_IRRIGATION_MODULE=true
VITE_ENABLE_SITE_MANAGEMENT_MODULE=true
VITE_ENABLE_EQUIPMENT_MODULE=true
VITE_ENABLE_ADMIN_MODULE=true